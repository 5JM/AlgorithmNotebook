   "source": [
    "# 재귀 연습 문제\n",
    "## 1. N부터 1까지 출력"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5\n",
      "4\n",
      "3\n",
      "2\n",
      "1\n"
     ]
    }
   ],
   "source": [
    "def printNto1(N):\n",
    "    if N == 0:\n",
    "        return\n",
    "    \n",
    "    print(N)\n",
    "    printNto1(N-1)\n",
    "\n",
    "printNto1(5)"
   ]
  },
  {
   "attachments": {},
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 2. 1부터 N까지 합"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "55\n"
     ]
    }
   ],
   "source": [
    "def sum1toN(N):\n",
    "    if N == 0:\n",
    "        return 0\n",
    "    \n",
    "    return N + sum1toN(N-1)\n",
    "\n",
    "print(sum1toN(10))"
   ]
  },
